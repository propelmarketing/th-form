<!DOCTYPE html>
<html>
<head>
<meta charset="utf-8" />
<title>Form for unit tests</title>
<meta name="viewport" content="width=device-width, initial-scale=1">
<script src="https://api.thrivehive.com/content/WebTrack/catracker.js"></script>
<script type="text/javascript">
try {
  var cat = new CATracker('4a4573812d7642c8ab7fad661c7f8db7');
  cat.Pause = true;
  cat.TrackOutboundLinks();
  cat.PageView();
} catch (err) {
  document.write('There has been an error initializing web tracking.');
}
</script>
</head>

<body>
  <h1>Form for unit tests</h1>
  <form autocomplete="on" action="/" method="POST" onSubmit="alert('submit')">
    <label class="caption">
      <input name="fname" type="text" maxlength="30">
      First Name
    </label>
    <label class="caption">
      <input name="lname" type="text" maxlength="30">
      Last Name
    </label>
    <label class="title" for="email">Email Address <span class="required">*</span></label>
    <input class="field-element" name="email" x-autocompletetype="email" type="text" spellcheck="false" id="email">
    <button onClick="alert('click')">Submit</button>
  </form>
</body>
</html>